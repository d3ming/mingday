{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "experimentalObjectRestSpread": true
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "warn"
  },
  "extends": ["prettier", "plugin:react/recommended"]
}
